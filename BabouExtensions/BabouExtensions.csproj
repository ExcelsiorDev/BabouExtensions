<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Version>1.2.0</Version>
        <PackageVersion>1.2.0</PackageVersion>
        <Title>Babou Extensions</Title>
        <Product>Babou Extensions</Product>
        <Description>Another Extension Methods package, but named after Babou and built in Fort Kickass from the tv show Archer.</Description>
        <Summary>Another Extension Methods package, but named after Babou and built in Fort Kickass from the tv show Archer.</Summary>
        <Authors>ajtatum</Authors>
        <Owners>ajtatum</Owners>
        <Copyright>ajtatum</Copyright>
        <Company>https://ajt.io</Company>
        <PackageId>BabouExtensions</PackageId>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PackageProjectUrl>https://github.com/ajtatum/BabouExtensions</PackageProjectUrl>
        <PackageIconUrl>https://raw.githubusercontent.com/ajtatum/BabouExtensions/master/assets/Babou.png</PackageIconUrl>
        <PackageTags>extension-methods extensions extension dotnet babou</PackageTags>
        <PackageReleaseNotes>Added string extension WithMaxLength as a replacement for Truncate. Moved HttpRequestExtensions to BabouExtensions.AspNetCore to remove dependency on Microsoft.AspNetCore.Http.Abstraction.</PackageReleaseNotes>
        <RepositoryUrl>https://github.com/ajtatum/BabouExtensions</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <NeutralLanguage>en-US</NeutralLanguage>
        <LangVersion>default</LangVersion>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
        <AssemblyVersion>1.2.0.0</AssemblyVersion>
        <FileVersion>1.2.0.0</FileVersion>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Update="NETStandard.Library" Version="2.0.0" />
    </ItemGroup>

</Project>