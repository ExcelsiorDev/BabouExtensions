<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.1.0</Version>
    <Title>Babou Extensions</Title>
    <Description>Another Extension Methods package, but named after Babou and built in Fort Kickass from the tv show Archer.</Description>
    <Summary>Another Extension Methods package, but named after Babou and built in Fort Kickass from the tv show Archer.</Summary>
    <Authors>ajtatum</Authors>
    <Owners>ajtatum</Owners>
    <Copyright>Copyright 2017</Copyright>
    <PackageId>BabouExtensions</PackageId>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageProjectUrl>https://github.com/ajtatum/BabouExtensions</PackageProjectUrl>
    <PackageTags>extension-methods extensions extension</PackageTags>
    <PackageVersion>1.0.0</PackageVersion>
    <RepositoryUrl>https://github.com/ajtatum/BabouExtensions</RepositoryUrl>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Product>Babou Extensions</Product>
    <PackageIconUrl>https://github.com/ajtatum/BabouExtensions/raw/master/Babou.png</PackageIconUrl>
    <PackageLicenseUrl>https://github.com/ajtatum/BabouExtensions</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Update="NETStandard.Library" Version="2.0.0" />
  </ItemGroup>

</Project>